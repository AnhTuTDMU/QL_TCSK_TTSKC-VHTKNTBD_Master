@model List<QL_ToChucSuKien_TTSKCĐVHTKNTBD_Master.Models.UsersModel>
<section id="speakers" class="speakers section">

    <!-- Section Title -->
    <div class="container section-title aos-init aos-animate" data-aos="fade-up">
        <h2>Event Speakers<br></h2>

    </div><!-- End Section Title -->

    <div class="container">

        <div class="row gy-3">

            <div class="col-xl-3 col-lg-3 col-md-6 aos-init aos-animate" data-aos="fade-up" data-aos-delay="100">
                <div class="member">
                    @foreach (var user in Model)
                    {
                        <img src="@user.ProfilePicture" class="img-fluid" alt="">
                        <div class="member-info">
                            <div class="member-info-content">
                                <h4><a href="speaker-details.html">@user.UserName</a></h4>
                                <span>Quas alias incidunt</span>
                            </div>
                            <div class="social">
                                <a href="https://www.facebook.com/AnhTu0803"><i class="bi bi-facebook"></i></a>
                                <a href="https://www.instagram.com/atus083"><i class="bi bi-instagram"></i></a>
                                <a href="https://github.com/AnhTuTDMU"><i class="bi bi-github"></i></a>
                            </div>
                        </div>
                    }
                </div>
            </div>
          
        </div>

    </div>

</section>