    <!-- Favicons -->
    <link href="~/uploads/Logo.jpg" rel="icon">
    <link href="~/uploads/Logo.jpg" rel="apple-touch-icon">

    <!-- Fonts -->
    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Raleway:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Vendor JS Files -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/index.js"></script>

    <link rel="stylesheet" href="~/css/index.css" />
</head>
<body class="d-flex flex-column h-100 scrolled">
    <header id="header" class="header d-flex align-items-center fixed-top">
        <div class="container-fluid container-xl position-relative d-flex align-items-center">

            <a href="@Url.Action("Index","Home")" class="logo d-flex align-items-center me-auto">
                <img src="~/uploads/Logo.jpg" alt="">
                <h6 style="color:  #2f3138;">Trung tâm sáng kiến cộng đồng và hỗ trợ khởi nghiệp Bình Dương</h6>
            </a>

            <nav id="navmenu" class="navmenu">
                <ul>
                    <li><a href="#hero" class="active">Trang chủ<br></a></li>
                    <li class="dropdown">
                        <a href="#"><span>Sự kiện</span> <i class="bi bi-chevron-down toggle-dropdown"></i></a>
                        <ul>
                            <li><a href="#">Tất cả sự kiện </a></li>
                            <li class="dropdown">
                                <a href="#EndedEvents"><span>Sự kiện sắp diễn ra</span> <i class="bi bi-chevron-down toggle-dropdown"></i></a>
                                <ul>
                                    <li><a href="#">Sự kiện ngoài trời</a></li>
                                    <li><a href="#">Sự kiện trong nhà</a></li>

                                </ul>
                            </li>
                            <li><a href="#EndEvents" class="active">Sự kiện đã kết thúc</a></li>
                        </ul>
                    </li>
                    <li><a href="#" class="">Dịch vụ<br></a></li>
                    <li><a href="#" class="">Ưu đãi<br></a></li>
                    <li><a href="#">Về chúng tôi</a></li>
             

                </ul>
                <i class="mobile-nav-toggle d-xl-none bi bi-list"></i>
            </nav>

        </div>
   
    </header>
    <div class="main body-content">
        @RenderBody()
        <div id="EndedEvents">
        </div>
        <div id="EndEvents">
        </div>
    </div>
    <footer id="footer" class="footer">

        <div class="footer-top">
            <div class="container">
                <div class="row gy-4">
                    <div class="col-lg-10 col-md-12 footer-about">
                        <a href="@Url.Action("Index","Home")" class="logo d-flex align-items-center">
                            <span class="sitename">
                                SỞ KHOA HỌC VÀ CÔNG NGHỆ TỈNH BÌNH DƯƠNG TRUNG TÂM SÁNG KIẾN CỘNG ĐỒNG VÀ HỖ TRỢ KHỞI NGHIỆP
                            </span>
           
                        </a>
                        <div class="footer-contact pt-3">
                            <p>Số 28, Huỳnh Văn Nghệ, P.Phú Lợi, TP.Thủ Dầu Một, tỉnh Bình Dương</p>
                            <p class="mt-3"><strong>Số điện thoại:</strong> <span>02743866969</span></p>
                            <p><strong>Email:</strong> <span>biic@binhduong.gov.vn</span></p>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="copyright text-center">
            <div class="container d-flex flex-column flex-lg-row justify-content-center justify-content-lg-between align-items-center">

                <div class="d-flex flex-column align-items-center align-items-lg-start">
                    <div>
                        © Copyright <strong><span>TTP</span></strong>.Đã đăng ký bản quyền
                    </div>
                </div>

                <div class="social-links order-first order-lg-last mb-3 mb-lg-0">
                    <a href=""><i class="bi bi-facebook"></i></a>
                    <a href=""><i class="bi bi-instagram"></i></a>
                    <a href="#"><i class="bi bi-envelope-fill"></i></a>
                    <a href="#" target="_blank"><i class="bi bi-github"></i></a>

                </div>

            </div>
        </div>

    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>



    @await RenderSectionAsync("Scripts", required: false)
    
</body>
</html>
